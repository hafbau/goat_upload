{
  "name": "upload_large_files",
  "version": "1.0.0",
  "description": "Upload large files",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server",
    "dev": "nodemon --watch 'src/**' --ext 'ts,json' --ignore 'src/**/*.spec.ts' --exec ts-node src/server",
    "test": "LOG_LEVEL=0 jest",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hafbau/upload_large_files.git"
  },
  "keywords": [
    "node stream",
    "large files",
    "upload"
  ],
  "author": "Hafiz Suara",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hafbau/upload_large_files/issues"
  },
  "homepage": "https://github.com/hafbau/upload_large_files#readme",
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "devDependencies": {
    "@types/jest": "26.0.22",
    "@types/node": "^15.0.3",
    "@types/supertest": "^2.0.11",
    "jest": "26.6.3",
    "supertest": "^6.1.3",
    "ts-jest": "26.5.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@types/express": "^4.17.11",
    "@types/nodemon": "^1.19.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.7"
  }
}
